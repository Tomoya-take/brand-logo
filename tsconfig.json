{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "outDir": "build",
    "noImplicitAny": false,
    "noEmitOnError": false,
    "baseUrl": "app",
    "paths": {
      "~/*": ["*"]
    }
  },
  "include": [
    "app/**/*",
    "server.ts",
    "webhooks.ts",
    "global.d.ts"
  ],
"exclude": [
  "node_modules"
]
}





